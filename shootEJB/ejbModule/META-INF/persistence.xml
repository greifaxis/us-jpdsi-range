<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" 
xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
<persistence-unit name="shootJPA" transaction-type="JTA">
<jta-data-source>java:/ShootDS</jta-data-source>
<class>com.jsf.entities.Gun</class>
<class>com.jsf.entities.Message</class>
<class>com.jsf.entities.Reservation</class>
<class>com.jsf.entities.User</class>
		
<exclude-unlisted-classes>false</exclude-unlisted-classes>
<properties>
<property name="hibernate.show_sql" value="true"/>
</properties>
</persistence-unit>
</persistence>